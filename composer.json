{
    "name": "cresenity/wago-php",
    "type": "library",
    "description": "The Official Wago PHP API Library",
    "keywords": ["wa-go", "api", "cresenity"],
    "homepage": "https://wa-go.id",
    "license": "MIT",
    "authors": [
      {
        "name": "Hery Kurniawan",
        "email": "hery.cresenity@gmail.com",
        "homepage": "https://github.com/heerrr"
      }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.3",
        "monolog/monolog": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
      "psr-4": { "Cresenity\\Vendor\\Wago\\": "src/" }
    },
    "autoload-dev": {
      "psr-4": { "Cresenity\\Vendor\\Wago\\Tests\\": "tests/" }
    },
    "config": {
      "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
